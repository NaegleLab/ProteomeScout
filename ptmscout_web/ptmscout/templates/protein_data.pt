<div metal:use-macro="layout">
	<div metal:fill-slot="header">
		<link rel="stylesheet" type="text/css" href="${request.static_url('ptmscout:static/css/proteins.css')}" />
	</div>
	<div metal:fill-slot="content">
		<div metal:use-macro="protein_header"></div>

		<div class="section">
			<div class="experiment_data" tal:repeat="experiment experiment_data">
				<div class="title">
					<a tal:attributes="href request.application_url + '/experiments/' + str(experiment['id'])" tal:content="experiment['title']"></a>
				</div>
				
				<div class="data" tal:repeat="data experiment['data']">
					<div class="run" tal:content="data['run']"></div>
					<div class="phosphopeps" tal:content="','.join(data['phosphopeps'])"></div>
					<div tal:repeat="row data['values']" tal:content="','.join(row)"></div>
				</div>
				
				<div class="chart"></div>
			</div>
		</div>
	</div>
</div>
